{
  "name": "hsql",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "better-npm-run start",
    "build:prod": "better-npm-run build:prod",
    "format": "better-npm-run format"
  },
  "betterScripts": {
    "start": {
      "command": "./purehmr",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "spago build &&  rm -rf  ../hastock/static/* && parcel build index.js --no-source-maps --public-url ./ --out-dir ../hastock/static --out-file index.js && cp ./index.html ../hastock/static/index.html",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "format": {
      "command": "purty --dynamic --write"
    }
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "better-npm-run": "^0.1.1",
    "purty": "^3.0.7"
  },
  "devDependencies": {
    "cssnano": "^4.1.10"
  }
}
